    "def uni_char(instr):\n",
    "    \n",
    "    l = len(instr)\n",
    "    if l == 0:\n",
    "        return None\n",
    "    \n",
    "    instr = sorted(instr)\n",
    "    i=0\n",
    "    \n",
    "    while i < l:\n",
    "        if instr[i] == instr[i+1]:\n",
    "            return False\n",
    "        else:\n",
    "            i += 1\n",
    "            \n",
    "    return True"
   ]
